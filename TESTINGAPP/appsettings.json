{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "logs/app.log",
      "Append": true,
      "MaxFileSize": 10485760,
      "MaxRollingFiles": 3
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=RecordDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  }

}
